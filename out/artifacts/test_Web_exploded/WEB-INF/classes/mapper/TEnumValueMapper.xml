<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.example.mp.test.web.mapper.TEnumValueMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.example.mp.test.web.entity.TEnumValue">
        <id column="id" property="id" />
        <result column="type_id" property="typeId" />
        <result column="enumValue" property="enumValue" />
        <result column="enumValueName" property="enumValueName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, type_id, enumValue, enumValueName
    </sql>

</mapper>
